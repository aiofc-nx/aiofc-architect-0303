{
  "name": "@aiofc/architect",
  "version": "0.0.0",
  "license": "MIT",
  "packageManager": "pnpm@8.15.0",
  "volta": {
    "node": "20.18.1"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "commit": "cz",
    "clean": "gulp clean:bundle",
    "format": "prettier --write \"**/src/**/*.ts\"",
    "lint": "eslint \"apps/**/*.ts\" \"packages/**/*.ts\" --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "cz-customizable.config.js"
    }
  },
  "private": true,
  "devDependencies": {
    "husky": "^8.0.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/types": "^19.5.0",
    "commitizen": "^4.3.1",
    "cz-customizable": "^7.4.0",
    "@darraghor/eslint-plugin-nestjs-typed": "^6.2.5",
    "@eslint/js": "^9.21.0",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.11",
    "@nx/eslint": "20.5.0-rc.1",
    "@nx/eslint-plugin": "20.5.0-rc.1",
    "@nx/jest": "20.5.0-rc.1",
    "@nx/js": "20.5.0-rc.1",
    "@nx/nest": "20.5.0-rc.1",
    "@nx/node": "20.5.0-rc.1",
    "@nx/web": "20.5.0-rc.1",
    "@nx/webpack": "20.5.0-rc.1",
    "@nx/workspace": "20.5.0-rc.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.14",
    "@types/node": "~18.16.9",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nx": "20.5.0-rc.1",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "ts-node": "10.9.2",
    "tslib": "^2.8.1",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.26.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.11",
    "@nestjs/core": "^11.0.11",
    "@nestjs/platform-express": "^11.0.11",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  }
